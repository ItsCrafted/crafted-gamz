#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules/acorn/bin/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules/acorn/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/node_modules:/mnt/c/Users/Bandi/Main/node_modules:/mnt/c/Users/Bandi/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules/acorn/bin/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules/acorn/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/node_modules:/mnt/c/Users/Bandi/Main/node_modules:/mnt/c/Users/Bandi/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Bandi/Main/All things Crafted/GitHub/crafted-gamz/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
